{
  "name": "messenger",
  "version": "1.0.0",
  "description": "Free to use online real time messenger build on Vite",
  "main": "main.ts",
  "type": "module",
  "engines": {
    "npm": ">=9.6.4",
    "node": "<=18.15.0"
  },
  "scripts": {
    "dev": "node entry-server.js",
    "build": "tsc && vite build",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.js"
  },
  "keywords": [
    "messeger",
    "real",
    "time"
  ],
  "author": "Vitalii Iakunin",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "handlebars": "^4.7.7",
    "postcss-nesting": "^11.2.2",
    "sass": "^1.62.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
